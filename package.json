{
  "name": "funquiz",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve-prod": "env-cmd .env.prod node dist/server.js",
    "serve": "env-cmd .env.dev node dist/server.js",
    "start-temp": "concurrently -c \"yellow.bold,green.bold\" -n \"SERVER,BUILD\" \"nodemon server\" \"ng build --watch\"",
    "build-ts": "npm run tslint && tsc --build tsconfig.server.json",
    "tslint": "tslint -p tsconfig.server.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "build": "env-cmd .env.dev ng build && npm run build-ts && npm run copy-static-assets",
    "build-prod": "env-cmd .env.prod ng build --prod && npm run build-ts && npm run copy-static-assets",
    "start": "npm run build-ts && npm run copy-static-assets && npm run serve-prod",
    "buildclient": "ng build --prod --output-path=dist",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "0.10.3",
    "@angular/animations": "7.0.1",
    "@angular/common": "7.0.1",
