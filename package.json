{
  "name": "funquiz",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve-prod": "env-cmd .env.prod node dist/server.js",
    "serve": "env-cmd .env.dev node dist/server.js",
    "start": "concurrently -c \"yellow.bold,green.bold\" -n \"SERVER,BUILD\" \"nodemon server\" \"ng build --watch\"",
    "build-ts": "npm run tslint && tsc --build tsconfig.server.json",
    "tslint": "tslint -p tsconfig.server.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "build": "env-cmd .env.dev ng build && npm run build-ts && npm run copy-static-assets",
    "build-prod": "env-cmd .env.prod ng build --prod && npm run build-ts && npm run copy-static-assets",
    "deploy-run": "ng build --prod && npm run build-ts && npm run copy-static-assets && npm run serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "angular-tree-component": "^8.2.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "core-js": "2.6.1",
    "cors": "^2.8.5",
    "env-cmd": "^8.0.2",
    "express": "^4.16.4",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.1",
    "jquery": "3.3.1",
    "log4js": "^3.0.6",
    "method-override": "^3.0.0",
    "mongoose": "^5.4.2",
    "morgan": "^1.9.1",
    "ng2-adsense": "5.4.0",
    "ngx-cookie-service": "2.1.0",
    "ngx-facebook": "2.4.0",
    "ngx-gauge": "1.0.0-beta.7",
    "nodemon": "^1.18.9",
    "rxjs": "6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.11.4",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "7.1.4",
    "@angular/language-service": "7.1.4",
    "@types/body-parser": "^1.17.0",
    "@types/compression": "0.0.36",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.16.0",
    "@types/helmet": "0.0.42",
    "@types/jasmine": "~3.3.5",
    "@types/jasminewd2": "~2.0.6",
    "@types/method-override": "0.0.31",
    "@types/mongoose": "^5.3.7",
    "@types/node": "~10.12.18",
    "@types/shelljs": "^0.8.1",
    "codelyzer": "4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.4",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "protractor": "^5.4.2",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0",
    "typescript": "^3.1.1"
  }
}
