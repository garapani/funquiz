{
  "name": "funquiz",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "serve": "node dist/server/server.js",
    "start": "concurrently -c \"yellow.bold,green.bold\" -n \"SERVER,BUILD\" \"nodemon server\" \"ng build --watch\"",
    "build-ts": "npm run tslint && tsc --build tsconfig.server.json",
    "tslint": "tslint -p tsconfig.server.json",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "build": "ng build && npm run build-ts && npm run copy-static-assets",
    "build-prod": "ng build --prod && npm run build-ts && npm run copy-static-assets",
    "deploy-run": "ng build --prod && npm run build-ts && npm run copy-static-assets && npm run serve",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
